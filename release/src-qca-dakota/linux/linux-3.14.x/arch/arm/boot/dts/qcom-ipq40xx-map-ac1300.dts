#include "qcom-ipq40xx-ap.dk01.1.dtsi"

/ {
	model = "ASUS. MAP-AC1300";

	memory {
		device_type = "memory";
		reg = <0x80000000 0x08000000>; /* 128MB */
	};

	soc {
		mdio@90000 {
			status = "ok";
			pinctrl-0 = <&mdio_pins>;
			pinctrl-names = "default";
			phy-reset-gpio = <&tlmm 62 0>;
			bias-pull-up;
		};

		gpio_keys {
			status = "disabled";
		};

		pinctrl@0x01000000 {
			i2c_0_pins: i2c_0_pinmux {
				mux {
					pins = "gpio58", "gpio59";
					function = "blsp_i2c0";
					bias-disable;
				};
			};

			serial_pins: serial_pinmux {
				mux {
					pins = "gpio60", "gpio61";
					function = "blsp_uart0";
				};
				rx {
					qcom,pins = "gpio60";
					drive-strength = <2>;
					bias-pull-down;
				};
				tx {
					qcom,pins = "gpio61";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};

		i2c_0: i2c@78b7000 { /* BLSP1 QUP2 */
			pinctrl-0 = <&i2c_0_pins>;
			pinctrl-1 = <&i2c_0_pins>;
			pinctrl-names = "i2c_active", "i2c_sleep";
			status = "ok";

			lp55231@32 {
				compatible = "national,lp5523";
				reg = <0x32>;
				clock-mode = /bits/ 8 <1>;

				chan0 {
					chan-name = "blue1";
					led-cur = /bits/ 8 <0x80>;
					max-cur = /bits/ 8 <0xff>;
				};

				chan1 {
					chan-name = "blue2";
					led-cur = /bits/ 8 <0x80>;
					max-cur = /bits/ 8 <0xff>;
				};

				chan2 {
					chan-name = "blue3";
					led-cur = /bits/ 8 <0x80>;
					max-cur = /bits/ 8 <0xff>;
				};

				chan3 {
					chan-name = "green1";
					led-cur = /bits/ 8 <0x80>;
					max-cur = /bits/ 8 <0xff>;
				};

				chan4 {
					chan-name = "green2";
					led-cur = /bits/ 8 <0x80>;
					max-cur = /bits/ 8 <0xff>;
				};

				chan5 {
					chan-name = "green3";
					led-cur = /bits/ 8 <0x80>;
					max-cur = /bits/ 8 <0xff>;
				};

				chan6 {
					chan-name = "red1";
					led-cur = /bits/ 8 <0x80>;
					max-cur = /bits/ 8 <0xff>;
				};

				chan7 {
					chan-name = "red2";
					led-cur = /bits/ 8 <0x80>;
					max-cur = /bits/ 8 <0xff>;
				};

				chan8 {
					chan-name = "red3";
					led-cur = /bits/ 8 <0x80>;
					max-cur = /bits/ 8 <0xff>;
				};
			};

		};

		spi_0: spi@78b5000 { /* BLSP1 QUP1 */
			status = "ok";
			qcom,gpio-cs1 = <&tlmm 1 0>;

			mt29f@1 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "spinand,mt29f";
				reg = <1>;
				spi-max-frequency = <24000000>;
			};

		};

		edma@c080000 {
			qcom,num-gmac = <3>;
			gmac0 {
				qcom,poll-required = <0>;
				qcom,poll-required-dynamic = <0>;
			};
			gmac1 {
				qcom,poll-required = <0>;
				qcom,poll-required-dynamic = <0>;
			};
			gmac2 {
				qcom,poll-required = <0>;
				qcom,poll-required-dynamic = <0>;
				vlan-tag = <3 0x00>;
			};
		};
	};
};
